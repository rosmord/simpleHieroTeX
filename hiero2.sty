\def\@Smaller{%
  \ifnum\@currentHieroglyphicBody=\minHieroglyphicSize%
  % DO NOTHING
  \else
  \advance\@currentHieroglyphicBody by 1%
  \advance\@currentHieroglyphicBodyW by 1%
  \advance\@currentHieroglyphicBodyH by 1%
  \advance\@Hauteur by 1%
