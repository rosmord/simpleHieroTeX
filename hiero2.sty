%% select a reduction factor for glyphs, depending on the value of
%% counter \@currentHieroglyphicBody
%% Reduction factors are indicated as numbers. 1 = full size,
%% 2 = divided by 1.2, 3 = divided by (1.2)^2, etc...
\def\selectHieroglyphSize#1{%
\ifcase#1
\fontsize{24}{24}%  0
\or\fontsize{16}{16}%1
\or\fontsize{12}{12}%2
\or\fontsize{8.5}{8.5}%3
\or\fontsize{6}{6}%4
\or\fontsize{4.25}{4.25}%5
\else
\message{Too small a sign!}%
\fi
\selectfont
}

