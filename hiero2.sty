\def\hieroglyphicParagraph#1{\bgroup
\def\@separator{}%
  \leavevmode
#1\relax
\egroup}

\def\@showQuadrant#1{%
  \@separator#1\def\@separator{\HinterCadratSpace}\ignorespaces%
}

